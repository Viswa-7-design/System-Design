package Singleton;

public class Synchronised {
	private static Synchronised instance;
	  private Synchronised() {
		  
	  }
	  public static Synchronised getInstance() {
		  if(instance==null) {
			  instance=new Synchronised();
		  }
		  return instance;
	  }
	  public void getFun() {
		  System.out.println("Synchronised Singleton Class");
	  }
}
